<h1 class = "text-lg-center title">Recent Questions (<%= @questions.length %>)</h1>
<section class="row questions">
  <div class="col-md-6 col-md-offset-3">
    <ul>
      <% @questions.each do |question| %>
        <li><%= link_to "#{question.title}", question_path(question) %></li>
        <hr>
      <% end %>
    </ul>
  </div>
</section>
